# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Database URL (for Drizzle ORM)
# Get this from Supabase: Settings > Database > Connection string
# Use the "Transaction pooler" URL (port 6543)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Anthropic AI (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key

# ABOSS Music (optional - for importing events/bookings)
# Get your API key from ABOSS: Profile Settings > Authentication Token
ABOSS_API_KEY=your_aboss_api_key
ABOSS_PROJECT_ID=your_aboss_project_id
ABOSS_AGENCY_ID=your_aboss_agency_id

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
